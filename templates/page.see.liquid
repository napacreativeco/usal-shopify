<div data-page--see class="page">

    <div class="topper">
        <div>Usal&reg; &nbsp; &nbsp; &nbsp; &nbsp; Los Angeles, California</div>
        <div>
            {{ "today" | date: "%B %d, %Y" }}
        </div>
    </div>
    <header>
        {% render 'navbar' %}
        {% render 'navbar--mobile' %}
    </header>
    
    <div class="anime-line">&nbsp;</div>

    <div class="content">

        <div class="video-archive">
            <div class="row">
                <div class="date-column">2022</div>

                <div class="video-loop">

                </div>
            </div>

            <div class="row">
                <div class="date-column">2021</div>

                <div class="video-loop">

                </div>
            </div>
        </div>

    </div>

</div>


<style>
    .topper {
        border-bottom: 2px solid var(--white);
    }
    footer,
    footer a  {
        color: var(--white) !important;
    }
    footer.desktop .row {
        border-top: 2px solid var(--white);
        border-bottom: 2px solid var(--white);
    }
    footer svg {
        fill: var(--white) !important;
    }
</style>

<script>
    var logoHolder = $('div[data-logo]');
    logoHolder.html('<h1>{{ page.title }}</h1>')
    (function() {

        $('body').css('background', '#191919');


    }(jQuery))
</script>